			th:style="'background-image: url(/assets/img/destination/img-' + ${iterStat.index + 1} + '.jpg)'"
			th:data-areaid="${area.aId}"
			th:data-hotelid="${hotels[iterStat.index].id}"
			onclick="location.href='/map?area=' + $(this).data('areaid') + '&amp;hotelId=' + $(this).data('hotelid')"
		>
			<div class="category-overlay">
				<div class="category-content">
					<h4 class="category-subtitle"><i class="flaticon-pin"></i> <span th:text="${area.aName}"></span></h4>
				</div>
			</div>
		</div>
	</div>
</div>
